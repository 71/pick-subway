{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "target": "ES2022",
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
  },
  "include": ["src"],
}
